//定义blocks,我们在这里定义三个block，一个是instruction，一个是练习block，一个是正式实验的block。
=============================================================================================================================================================



<block judgeblk>
	/screencolor = (0,0,0)
	/blockfeedback = (meanlatency,correct)
	/trials = [1=pic1;2=pic2;3=pic3;4=pic4;5=pic5;6=pic6;7=pic7;8=pic8;9=pic9;10=pic10;11=pic11;12=pic12;13=pic13;14=pic14;15=pic15]
</block>


//定义experiment, 加入blocks
======================================================================================================================================================================
<expt>
	/preinstructions = (begininstruction)
	/ blocks = [1 = judgeblk]
	/postinstructions = (endinstruction)
</expt>
