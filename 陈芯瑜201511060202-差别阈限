 <item correctmsg>
 	/1 = "√"
 </item>
 
 <item errormsg>
 	/1 = "×"
 </item>
 
 <item fixation>
 	/1 = "+"
 </item>
 
 <text fixation>
 	/items = fixation
 	/select = 1
 	/fontstyle = ("Arial", 80ptc)
 	/txbgcolor = (transparent)
 	/txcolor = (255,255,255)
 </text>

 <text instruct>
 	/hjustify = center
 	/vposition = 50
 	/items = ("欢迎参加实验！每次会为您同时呈现四幅图，仔细观察哪幅图与其余图片灰度不同，请用鼠标点击该图。请按空格键开始实验。")
 	/size = (1000,300)
 	/fontstyle = ("宋体", 24pt)
 	/txbgcolor = (transparent)
 	/txcolor = (255,255,255)
 </text>
 
 <text goodbye>
 	/hjustify = center
 	/vposition = 50
 	/items = ("非常感谢您的参与，您提供的行为数据对我们的研究特别重要!")
 	/size = (1000,300)
 	/fontstyle = ("宋体", 24pt)
 	/txbgcolor = (transparent)
 	/txcolor = (255,255,255)
 </text>
 
 
 <text correctmsg>
	/items = correctmsg
 	/select = 1
 	/fontstyle = ("楷体", 60ptc, false, false, false, false, 5, 1)
 	/txcolor = (255,255,255)
 	/txbgcolor = (0, 0, 0)
 </text>
 
 <text errormsg>
