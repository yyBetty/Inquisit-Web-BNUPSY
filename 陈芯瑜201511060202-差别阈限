 //定义trials
 <trial instruct>
 	/validresponse = (" ")
 	/stimulustimes = [1=instruct]
 	/recorddata = false
 </trial>
 
 <trial goodbye>
 	/validresponse = ("q")
 	/stimulustimes = [1=goodbye]
 	/recorddata = false
 </trial>
 
 <trial npic00>
 	/pretrialpause = 100
 	/correctmessage = (correctmsg,500)
 	/errormessage = (errormsg,500)
 	/inputdevice = mouse
 	/stimulusframes = [1=fixation;50 = N0,M2,M3,M4,black]
 	/validresponse = (lbuttondown)
 	/correctresponse = (N0)
 </trial>
 
 <trial npic01>
 	/pretrialpause = 100
 	/correctmessage = (correctmsg,500)
