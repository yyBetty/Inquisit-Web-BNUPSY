<block pic10>
/ branch = [if (block.pic10.percentcorrect > 50) block.pic5]
/ branch = [if (block.pic10.percentcorrect < 50) block.endline]
/ blockfeedback = (correct)
/ trials = [1-8 = noreplace(2a1,2a2,2a3,2a4,2b1,2b2,2b3,2b4)]
</block>

<block pic5>
/ branch = [if (block.pic10.percentcorrect < 50) block.endline]
/ trials = [1-8 = noreplace(1a1,1a2,1a3,1a4,1b1,1b2,1b3,1b4)]
/ blockfeedback = (correct)
</block>

<block endline>
/ trials = [1 = endline]
</block>

<expt myexp>
/ preinstructions = (instruction)
/ blocks = [1 = pic10;2 = pic5;3 = endline]
</expt>
